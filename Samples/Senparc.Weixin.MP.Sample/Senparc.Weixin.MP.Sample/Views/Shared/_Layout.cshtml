        {
            <div class="catalog">
                <a href="@Url.Action("Index", "Menu")">@*自定义*@菜单设置工具</a>
                <a href="@Url.Action("Index", "SimulateTool")">消息模拟工具</a>
                <a href="@Url.Action("Test", "Cache")">缓存测试</a>
